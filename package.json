{
  "name": "chrom-graph",
  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "build:serve": "yarn serve dist",
    "watch": "webpack --watch",
    "serve": "webpack serve",
    "start": "yarn serve",
    "test": "jest",
    "test:ci": "yarn build && yarn test",
    "docker": "docker buildx build -t ghcr.io/erosson/chrom-graph:latest .",
    "docker:buildrun": "yarn docker && yarn docker:run",
    "docker:run": "docker run -p 8000:80 -it ghcr.io/erosson/chrom-graph:latest"
  },
  "dependencies": {
    "fp-ts": "^2.13.1",
    "graphology": "^0.25.1",
    "graphology-gexf": "^0.10.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.8.0",
    "sigma": "^2.4.0"
  },
  "devDependencies": {
    "@types/jest": "^29.4.0",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@types/sigmajs": "^1.0.28",
    "css-loader": "^6.7.3",
    "graphology-types": "^0.24.7",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.4.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.3",
    "serve": "^14.2.0",
    "style-loader": "^3.3.1",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.2",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^4.11.1"
  }
}
